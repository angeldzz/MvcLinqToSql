@model List<Empleado>
<h1>Empleado Linq To Sql</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Apellido</th>
            <th>Oficio</th>
            <th>Salario</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (Empleado empleado in Model)
{
            <tr>
                <td>@empleado.Apellido</td>
                <td>@empleado.Oficio</td>
                <td>@empleado.Salario</td>
                <td>
                    <a asp-action="Details" asp-route-idempleado="@empleado.IdEmpleado" class="btn btn-primary">Detalles</a>
                </td>
            </tr>
        }
    </tbody>
</table>